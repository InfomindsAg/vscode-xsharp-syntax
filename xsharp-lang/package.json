{
    "name": "xsharp-lang",
    "displayName": "X# Lang",
    "description": "Syntax highlighting for X# lang",
    "version": "1.0.0",
    "publisher": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/VolkmarR/vscode-xsharp-syntax.git"
    },
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xsharp",
            "aliases": ["X#", "xsharp"],
            "extensions": [".prg"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xsharp",
            "scopeName": "source.prg",
            "path": "./syntaxes/xsharp.tmLanguage.json"
        }]
    }
}